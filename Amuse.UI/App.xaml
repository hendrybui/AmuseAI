        <DataTemplate DataType="{x:Type models:ImageResult}">
            <Grid>

                <!--Progress Element-->
                <DockPanel HorizontalAlignment="Center" UseLayoutRounding="True" Visibility="{Binding Image, Converter={StaticResource InverseNullVisibilityConverter}}">
