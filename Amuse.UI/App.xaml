                            <Button Command="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type Window}}, Path=NavigateCommand}"  >
                                <userControls:FontAwesome Icon="&#xf008;" IconStyle="Light" Size="13" ToolTip="Send To ImageToVideo"/>
                                <Button.CommandParameter>
