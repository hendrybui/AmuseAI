                                </Button.CommandParameter>
                            </Button>
                            <Button Command="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type Window}}, Path=NavigateCommand}" BorderThickness="0, 1">
                                <userControls:FontAwesome Icon="&#xf03e;" IconStyle="Light" Size="13" ToolTip="Send To Image To Image"/>
                                <Button.CommandParameter>
                                    <MultiBinding Converter="{StaticResource NavigationConverter}">
                                        <Binding Source="{x:Static models:MenuId.Image}" />
                                        <Binding Source="{x:Static models:ImageSubmenuId.ImageToImage}" />
                                        <Binding Path="."/>
                                    </MultiBinding>
                                </Button.CommandParameter>
