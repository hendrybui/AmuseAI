                                <userControls:FontAwesome Icon="&#xf2ed;" IconStyle="Light" Size="13" ToolTip="Remove Image" Color="{StaticResource DangerColour}"/>
                            </Button>
                        </UniformGrid>
                    </StackPanel>
                    <Button Command="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type views:INavigatable}}, Path=PreviewImageCommand}" CommandParameter="{Binding}"  Margin="0,2"  BorderBrush="{StaticResource ContainerBorder}" BorderThickness="1" Background="{StaticResource ContainerBackground}" >
