                            <Button Command="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type views:INavigatable}}, Path=RemoveVideoCommand}" CommandParameter="{Binding}">
                                <userControls:FontAwesome Icon="&#xf2ed;" IconStyle="Light" ToolTip="Remove Video" Color="{StaticResource DangerColour}"/>
                            </Button>
                        </UniformGrid>
                    </StackPanel>
                    <Button Command="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type views:INavigatable}}, Path=PreviewVideoCommand}" CommandParameter="{Binding}" Margin="0,2" BorderBrush="{StaticResource ContainerBorder}" BorderThickness="1" Background="{StaticResource ContainerBackground}" >
