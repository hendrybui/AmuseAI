        <DataTemplate DataType="{x:Type models:VideoResultModel}">

            <Grid>

                <!--Progress Element-->
                <DockPanel HorizontalAlignment="Center" UseLayoutRounding="True" Visibility="{Binding Video, Converter={StaticResource InverseNullVisibilityConverter}}">
                    <Grid DockPanel.Dock="Bottom"  DataContext="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type views:ViewBase}}}" Margin="0,0,0,2">
                        <ProgressBar
                            Height="20"
                            Value="{Binding Progress.Value}" 
