<local:BaseDialog x:Class="Amuse.UI.Dialogs.UpdateModelDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Amuse.UI.Dialogs"
        xmlns:userControls="clr-namespace:Amuse.UI.UserControls"
        mc:Ignorable="d"
        x:Name="UI"
        Icon="/Images/Icon.png"
        MinWidth="1000"
        MaxWidth="1000"
        SizeToContent="WidthAndHeight"
        WindowStartupLocation="CenterOwner" 
        SnapsToDevicePixels="True"
        AllowsTransparency="True"
        WindowStyle="None"
        Style="{StaticResource BaseToolWindow}"
        Title="Update Model">
    <DockPanel DataContext="{Binding ElementName=UI}" Margin="5">

        <StackPanel  DockPanel.Dock="Left" >

            <GroupBox Header="Settings">
                <StackPanel Margin="5" >

                    <StackPanel Margin="0,0,4,0">
                        <TextBlock Text="Name" />
                        <TextBox Text="{Binding UpdateModelSet.Name}"  />
                    </StackPanel>

                    <UniformGrid Columns="2" Margin="0,5,0,0">

                        <StackPanel Margin="0,0,4,0">
                            <TextBlock Text="Pipeline" />
                            <ComboBox ItemsSource="{Binding Source={StaticResource PipelineType}}" SelectedItem="{Binding UpdateModelSet.PipelineType}" />
                        </StackPanel>

                        <StackPanel Margin="0,0,4,0">
                            <TextBlock Text="Model Type" />
                            <ComboBox ItemsSource="{Binding Source={StaticResource ModelType}}" SelectedItem="{Binding UpdateModelSet.ModelType}" />
                        </StackPanel>
                        <StackPanel Margin="0,0,4,0">
                            <TextBlock Text="Sample Size" />
                            <TextBox Text="{Binding UpdateModelSet.SampleSize}" />
                        </StackPanel>

                    </UniformGrid>

                    <UniformGrid Columns="2" Margin="0,5,0,0">
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableTextToImage}"  Content="Text To Image" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableImageToImage}"  Content="Image To Image" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableImageInpaintLegacy}" Content="Image Inpaint Legacy" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableImageInpaint}"  Content="Image Inpaint" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableControlNet}" Content="ControlNet" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableControlNetImage}"  Content="ControlNet Image" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableControlNetVideo}"  Content="ControlNet Video" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableTextToVideo}"  Content="Text To Video" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableImageToVideo}"  Content="Image To Video" VerticalAlignment="Center" Margin="0,4,0,0"/>
                        <CheckBox IsChecked="{Binding UpdateModelSet.EnableVideoToVideo}"  Content="Video To Video" VerticalAlignment="Center" Margin="0,4,0,0"/>
                    </UniformGrid>

                </StackPanel>
            </GroupBox>

            <GroupBox Margin="0,10,0,0">
                <StackPanel>
                    <TextBlock Text="Execution Device" Style="{StaticResource FieldTextBlockStyle}"/>
                    <userControls:DevicePickerControl
                         DeviceId="{Binding UpdateModelSet.DeviceId, Mode=TwoWay}"
                         Provider="{Binding UpdateModelSet.ExecutionProvider, Mode=TwoWay}" />
                </StackPanel>
            </GroupBox>

            <GroupBox Header="Advanced Settings" Margin="0,10">
                <UniformGrid Columns="3" Margin="5" VerticalAlignment="Top">

                    <!--<StackPanel Margin="0,0,4,0">
                        <TextBlock Text="Scale Factor" />
                        <TextBox Text="{Binding UpdateModelSet.ScaleFactor}" />
                    </StackPanel>-->
                    <StackPanel Margin="0,0,4,0">
                        <TextBlock Text="Tokenizer Limit" />
                        <TextBox Text="{Binding UpdateModelSet.TokenizerLimit}" />
                    </StackPanel>
                    <StackPanel Margin="0,0,4,0">
                        <TextBlock Text="Tokenizer2 Limit" />
                        <TextBox Text="{Binding UpdateModelSet.Tokenizer2Limit}" />
                    </StackPanel>
                    <StackPanel Margin="0,0,4,0">
                        <TextBlock Text="Tokenizer Length" />
                        <TextBox Text="{Binding UpdateModelSet.TokenizerLength}" />
                    </StackPanel>
                    <StackPanel Margin="0,3,4,0">
                        <TextBlock Text="Tokenizer2 Length" />
                        <TextBox Text="{Binding UpdateModelSet.Tokenizer2Length}" />
                    </StackPanel>
                    <StackPanel Margin="0,3,4,0">
                        <TextBlock Text="Tokenizer3 Length" />
                        <TextBox Text="{Binding UpdateModelSet.Tokenizer3Length}" />
                    </StackPanel>

                    <StackPanel Margin="0,3,4,0">
                        <TextBlock Text="Pad Token Id" />
                        <TextBox Text="{Binding UpdateModelSet.PadTokenId}" />
                    </StackPanel>
                    <StackPanel Margin="0,3,4,0">
                        <TextBlock Text="Blank Token Id" />
                        <TextBox Text="{Binding UpdateModelSet.BlankTokenId}" />
                    </StackPanel>

                    <StackPanel Margin="0,3,4,0">
                        <TextBlock Text="ContextSize" />
                        <TextBox Text="{Binding UpdateModelSet.ContextSize}" />
                    </StackPanel>

                </UniformGrid>
            </GroupBox>

        </StackPanel>


        <DockPanel DockPanel.Dock="Bottom"  Margin="0,20,0,0" >
            <UniformGrid DockPanel.Dock="Right" Columns="2" Height="30">
                <Button Content="Save" Command="{Binding SaveCommand}" IsDefault="True"/>
                <Button Content="Cancel" Command="{Binding CancelCommand}" Width="100"/>
            </UniformGrid>

            <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center">
                <Border BorderBrush="Red" BorderThickness="1" CornerRadius="4">
                    <StackPanel Orientation="Horizontal" Margin="30, 5">
                        <userControls:FontAwesome Color="Red" Icon="&#xf00d;" />
                        <TextBlock Text="{Binding ValidationError}" Margin="5,0,0,3" />
                    </StackPanel>
                    <Border.Style>
                        <Style TargetType="{x:Type Border}">
                            <Setter Property="Visibility" Value="Visible" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ValidationError}" Value="{x:Null}">
                                    <Setter Property="Visibility" Value="Collapsed" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Border.Style>
                </Border>
            </StackPanel>
        </DockPanel>


        <!--Model Files-->
        <StackPanel Margin="10,0,0,0">

            <GroupBox Header="Unet Model" >

                <DockPanel Margin="5" VerticalAlignment="Top">

                    <!--File Picker-->
                    <DockPanel DockPanel.Dock="Top">
                        <StackPanel DockPanel.Dock="Right" VerticalAlignment="Center">
                            <CheckBox IsChecked="{Binding UpdateModelSet.UnetModel.IsOverrideEnabled}" Content="Performance Override" Margin="5,0"/>
                        </StackPanel>
                        <StackPanel>
                            <userControls:FilePickerTextBox 
                           FileName="{Binding UpdateModelSet.UnetModel.OnnxModelPath, Mode=TwoWay}"  
                           IsRequired="True" 
                           Filter="Onnx files (*.onnx)|*.onnx"
                           Title="Model Selector" 
                           DefaultExt="onnx"/>
                        </StackPanel>
                    </DockPanel>

                    <!--Override Defaults-->
                    <Grid Grid.Column="0" Visibility="{Binding UpdateModelSet.UnetModel.IsOverrideEnabled, Converter={StaticResource BooleanToVisibilityConverter}}" Margin="0,5,0,0">
                        <StackPanel>
                            <TextBlock Text="Execution Device" Style="{StaticResource FieldTextBlockStyle}"/>
                            <userControls:DevicePickerControl
                                 DeviceId="{Binding UpdateModelSet.UnetModel.DeviceId, Mode=TwoWay}"
                                 Provider="{Binding UpdateModelSet.UnetModel.ExecutionProvider, Mode=TwoWay}" />
                        </StackPanel>
                    </Grid>

                </DockPanel>
            </GroupBox>

